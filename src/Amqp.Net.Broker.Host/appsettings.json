{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "AmqpBroker": {
    "Name": "amqp-broker",
    "Host": "0.0.0.0",
    "Port": 5672,
    "MaxConnections": 10000,
    "MaxFrameSize": 262144,
    "IdleTimeoutMs": 60000,
    "EnableManagementApi": true,
    "ManagementPort": 15672
  },
  "AllowedHosts": "*"
}
